Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/***
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes

Host **homelab ip addr**
  HostName **homelab ip addr**
  IdentityFile ~/.ssh/***
  User homelab
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes

Host *
  IdentityFile ~/.ssh/***
  IdentityFile ~/.ssh/***
  AddKeysToAgent yes
